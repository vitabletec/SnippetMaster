  "Register Custom Post Type": {
    "prefix": "registercpt",
    "body": [
      "function ${1:custom_post_type_name}_init() {",
      "\t$labels = array(",
      "\t\t'name' => _x( '${2:Custom Post Type Plural Name}', '${1:custom_post_type_name}' ),",
      "\t\t'singular_name' => _x( '${3:Custom Post Type Singular Name}', '${1:custom_post_type_name}' ),",
      "\t\t'add_new' => __( 'Add New', '${1:custom_post_type_name}' ),",
      "\t\t'add_new_item' => __( 'Add New ${3:Custom Post Type Singular Name}', '${1:custom_post_type_name}' ),",
      "\t\t'edit_item' => __( 'Edit ${3:Custom Post Type Singular Name}', '${1:custom_post_type_name}' ),",
      "\t\t'new_item' => __( 'New ${3:Custom Post Type Singular Name}', '${1:custom_post_type_name}' ),",
      "\t\t'all_items' => __( 'All ${2:Custom Post Type Plural Name}', '${1:custom_post_type_name}' ),",
      "\t\t'view_item' => __( 'View ${3:Custom Post Type Singular Name}', '${1:custom_post_type_name}' ),",
      "\t\t'search_items' => __( 'Search ${2:Custom Post Type Plural Name}', '${1:custom_post_type_name}' ),",
      "\t\t'not_found' => __( 'No ${2:Custom Post Type Plural Name} found', '${1:custom_post_type_name}' ),",
      "\t\t'not_found_in_trash' => __( 'No ${2:Custom Post Type Plural Name} found in Trash', '${1:custom_post_type_name}' ),",
      "\t\t'parent_item_colon' => '',",
      "\t\t'menu_name' => __( '${2:Custom Post Type Plural Name}', '${1:custom_post_type_name}' )",
      "\t);",
      "\t$args = array(",
      "\t\t'labels' => $labels,",
      "\t\t'public' => true,",
      "\t\t'has_archive' => true,",
      "\t\t'rewrite' => array( 'slug' => '${4:custom-post-type-slug}' ),",
      "\t\t'supports' => array( 'title', 'editor', 'thumbnail' ),",
      "\t);",
      "\tregister_post_type( '${1:custom_post_type_name}', $args );",
      "}",
      "add_action('init', '${1:custom_post_type_name}_init');"
    ],
    "description": "Registers a new custom post type in WordPress."
  },
  "Add Custom Meta Box": {
    "prefix": "addmetabox",
    "body": [
      "function ${1:custom_meta_box_name}_meta_box_callback($$post) {",
      "\t// Add your custom meta box code here.",
      "}",
      "function ${1:custom_meta_box_name}_add_meta_boxes() {",
      "\tadd_meta_box('${1:meta_box_id}', '${2:Meta Box Title}', '${1:custom_meta_box_name}_meta_box_callback', '${3:post_type}', '${4:context}', '${5:priority}');",
      "}",
      "add_action('add_meta_boxes', '${1:custom_meta_box_name}_add_meta_boxes');",
      "",
      "// Save custom meta box data",
      "function ${1:custom_meta_box_name}_save_meta_box_data($$post_id) {",
      "\t// Check if nonce is set.",
      "\tif ( ! isset( $$_POST['${1:meta_box_id}'] ) ) {",
      "\t\treturn;",
      "\t}",
      "\t// Verify that the nonce is valid.",
      "\tif ( ! wp_verify_nonce( $$_POST['${1:meta_box_id}'], '${1:custom_meta_box_name}' ) ) {",
      "\t\treturn;",
      "\t}",
      "\t// Check if the current user has permission to edit the post.",
      "\tif ( ! current_user_can( 'edit_post', $$post_id ) ) {",
      "\t\treturn;",
      "\t}",
      "\t// Update custom meta box data.",
      "\t// Add your custom meta box data update code here.",
      "}",
      "add_action('save_post', '${1:custom_meta_box_name}_save_meta_box_data');"
    ],
    "description": "Adds a custom meta box to a post edit screen in WordPress. The meta box can be used to capture additional information about the post, which can be displayed or used in the theme or other plugins."
  },
